import numpy  

# here goes the AI algorithm code for enemy 7 
class enemy7_controller(): 


        def __init__(self):
 
            self.atack1 = None
            self.atack2 = None
            self.atack3 = None
            self.atack4 = None
            self.atack5 = None
            self.atack6 = None


        def play(self,params):


            self.atack1 = numpy.random.choice([1,0])
            self.atack2 = numpy.random.choice([1,0])
            self.atack3 = numpy.random.choice([1,0])
	    self.atack4 = numpy.random.choice([1,0])
            self.atack5 = numpy.random.choice([1,0])
            self.atack6 = numpy.random.choice([1,0])


	    return self.atack1, self.atack2, self.atack3, self.atack4, self.atack5, self.atack6

	 
