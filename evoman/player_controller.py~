import numpy  
 
 
# Here goes the AI algorithm code.
class player_controller(): 


        def __init__(self):
 
            self.left = None
            self.right = None
            self.jump = None
            self.shoot = None
            self.release = None

        def play(self,params):


            self.left = numpy.random.choice([1,0])
            self.right = numpy.random.choice([1,0])
            self.jump = numpy.random.choice([1,0])
            self.shoot = numpy.random.choice([1,0])
            self.release = numpy.random.choice([1,0])
	

	    return self.left, self.right, self.jump, self.shoot, self.release

	 
